<Page x:Class="Project.HelloPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Project"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="HelloPage" Background="white">
    <Grid Loaded="Grid_Loaded">
        <StackPanel Margin="150,0,150,0" VerticalAlignment="Center">
            <TextBlock HorizontalAlignment="Center" Margin="10" TextWrapping="Wrap" Text="Прокат автомобилей"  />
            <Button   Content="Клиенты" x:Name="ToClient" Click="ToClient_Click" Margin="10" Height="30"/>
            <Button  Content="Список авто" x:Name="ToAuto" Click="ToAuto_Click"  Margin="10" Height="30"/>
            <Button  Content="Прокат авто" x:Name="ToProcat" Click="ToProcat_Click" Margin="10" Height="30"/>
            <Button  Content="Выход" x:Name="ReLoging" Click="ReLoging_Click" Margin="10" Height="25" Width="60"/>
        </StackPanel>





    </Grid>
    
    
</Page>
